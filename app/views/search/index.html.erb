
<h1>Searching for "<%= params[:q]%>"</h1>
<div style="width: 100%">
<%if(@entries.count) > 0 %>
  <div class="table-responsive">
    <table class="table table-striped table-hover dt-responsive display nowrap" cellspacing="0" id="EntryTable">
    <thead>
    <tr>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Pink</th>
      <th>Yellow</th>
      <th>Age</th>
      <th>Parent</th>
      <th>Guard</th>
    <th>Test Date</th>
      
    </tr>
    </thead>
    <tbody>
    <%= render 'shared/entry'%>
    </tbody>
    </table>
  </div>
  <%else%>
    <div class="alert alert-danger">No Matches Found</div>
  <%end%>
</div>

<br>
